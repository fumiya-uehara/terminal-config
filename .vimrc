set number
set cursorline
set cursorcolumn
set laststatus=2
set expandtab
set tabstop=2
set shiftwidth=2
set ignorecase
set smartcase
set incsearch
set wrapscan
set hlsearch
set autoindent
set showcmd
set backspace=indent,eol,start
set ruler
set shell=/bin/zsh
set clipboard+=unnamed
nnoremap j gj
nnoremap k gk
cnoremap <C-a> <Home>
cnoremap <C-e> <End>
inoremap <C-a> <Home>
inoremap <C-e> <End>
inoremap <C-f> <Right>
inoremap <C-b> <Left>
syntax on
